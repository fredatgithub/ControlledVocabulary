        <Label Height="28" HorizontalAlignment="Left" Margin="235,298,0,0" Name="labelOutookRunning" VerticalAlignment="Top" Foreground="#FFD81919" Content="Please ensure Outlook is closed." ClipToBounds="True" />
    </Grid>
</Window>
